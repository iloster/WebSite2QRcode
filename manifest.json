{  
  "name": "WebSite2QRcode",  
  "version": "1.0",  
  "description": "将网址转化成二维码",  
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "QRcode",
    "default_popup": "popup.html"
  } ,
  "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*", 
        "notifications"
      ],
    
   "manifest_version": 2  
} 